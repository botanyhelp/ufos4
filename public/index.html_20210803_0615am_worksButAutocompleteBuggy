<!-- 
    NO..feathersjs has some routing..so we put code in this page
         Import App <script src="./main.js"></script>

We get different records with URLs like these:
http://li1077-179.members.linode.com:3030/ufosightings?$limit=2&$skip=44755
-->
    <script>

const app = Vue.createApp({
  mounted() {
    let awesomeplete_listener = document.createElement('script')
    awesomeplete_listener.setAttribute('src', 'awesomeplete_listener.js')
    document.head.appendChild(awesomeplete_listener)
  },
  data() {
    return { 
        currentId: 1,
        showHome: false,
        showSearch: false,
        showNextTen: true,
        ufosightings: []
    }
  },
  created() {
    fetch('http://li1077-179.members.linode.com:3030/ufosightings')
    .then(response => response.json())
    .then(json => {
      this.ufosightings = json.data
    })
  },
  methods: {
    showSearchForm() {
      this.showSearch = !this.showSearch;
      this.showNextTen = !this.showNextTen;
    },
    onSubmitYear() {
      //YEAR
      alert("YEAR: "+this.sighted_at_year)
      fetch('http://li1077-179.members.linode.com:3030/ufosightings?sighted_at_year='+this.sighted_at_year)
      .then(response => response.json())
      .then(json => {
        this.ufosightings = json.data
      })
    },
    onSubmitState() {
      //STATE
      alert("STATE: "+this.location_state)
      fetch('http://li1077-179.members.linode.com:3030/ufosightings?location_state='+this.location_state)
      .then(response => response.json())
      .then(json => {
        this.ufosightings = json.data
      })
    },
    onSubmitCity() {
      //CITY
      alert("CITY: "+this.location_city.split(',')[0]);
      //fetch('http://li1077-179.members.linode.com:3030/ufosightings?location_city='+this.location_city)
      //fetch('http://li1077-179.members.linode.com:3030/ufosightings?location_city='+escape(this.location_city))
      //let url = 'http://li1077-179.members.linode.com:3030/ufosightings?location_city='+encodeURI(this.location_city);
      //alert("url: "+url);
      //fetch(url)
      fetch('http://li1077-179.members.linode.com:3030/ufosightings?location_city='+encodeURI(' '+this.location_city.split(',')[0]))
      .then(response => response.json())
      .then(json => {
        this.ufosightings = json.data
      })
    },
    nextTen() {
      this.currentId = this.currentId+10;
      fetch('http://li1077-179.members.linode.com:3030/ufosightings?$limit=10&$skip='+this.currentId)
      .then(response => response.json())
      .then(json => {
        this.ufosightings = json.data
      })
    }
  }
})
      const mountedApp = app.mount('#app')
    </script>
  </body>
</html>


